{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2017", "dom"],
    "jsx": "react",
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "baseUrl": "./src",
    "paths": {
      "styles": ["client/styles"],
      "styles/*": ["client/styles/*"],
      "components/*": ["client/components/*"],
      "routes/*": ["client/routes/*"],
      "models/*": ["models/*"],
      "utilities/*": ["utilities/*"],
      "recordize": ["recordize/*"],
      "client/*": ["client/*"],
      "server/*": ["server/*"]
    }
  }
}
